{
  "name": "ircv3",
  "description": "Connect to and interact with IRC servers. IRCv3 compatible.",
  "version": "0.19.3",
  "main": "lib",
  "types": "lib/index",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/d-fischer/ircv3.git"
  },
  "scripts": {
    "lint": "eslint --ext js,ts src",
    "prettier:check": "prettier --check 'src/**'",
    "prettier:fix": "prettier --write 'src/**'",
    "test": "jest",
    "build": "tsc",
    "rebuild": "rimraf lib && yarn run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@d-fischer/eslint-config": "^1.0.3",
    "@types/jest": "^24.0.17",
    "eslint": "^6.1.0",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "jest-environment-node": "^24.8.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "~3.7.2"
  },
  "dependencies": {
    "@d-fischer/logger": "^0.2.0",
    "@d-fischer/typed-event-emitter": "^2.0.0",
    "@types/clone": "^0.1.30",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/node": "^12.12.12",
    "@types/ws": "^6.0.3",
    "clone": "^2.1.2",
    "escape-string-regexp": "^2.0.0",
    "universal-websocket-client": "^1.0.1",
    "ws": "^7.2.0"
  }
}
